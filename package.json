{
  "name": "@nutriot/bandcamp-api",
  "version": "0.4.1",
  "description": "Node.js library for the Bandcamp API",
  "main": "lib/index.cjs",
  "exports": {
    "import": "./lib/index.mjs",
    "require": "./lib/index.cjs"
  },
  "files": [
    "lib/",
    "types/",
    "LICENSE",
    "README.md"
  ],
  "typings": "./types",
  "repository": {
    "type": "git",
    "url": "https://github.com/nutriot/bandcamp-api.git"
  },
  "keywords": [
    "bandcamp",
    "bandcamp-api"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rollup --config",
    "dev": "npm run start",
    "fix": "eslint --fix ./src",
    "lint:code": "eslint ./src --ignore-path .gitignore",
    "lint": "npm-run-all --parallel lint:*",
    "start": "rollup --watch --config",
    "test": "ava ./tests/*.mjs --serial --verbose",
    "prepack": "npm run build",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/node": "^14.18.33",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "ava": "^4.3.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.46.0",
    "eslint-plugin-json": "^3.1.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "isomorphic-fetch": "^3.0.0",
    "lint-staged": "^13.2.3",
    "npm-run-all2": "^6.0.6",
    "prettier": "^2.7.1",
    "remark-cli": "^11.0.0",
    "remark-preset-lint-recommended": "^6.1.2",
    "remark-preset-prettier": "^2.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.27.0",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "*.(json|js|ts)": "eslint --cache --fix",
    "*.md": "prettier --write"
  }
}
