{
  "name": "@nutriot/bandcamp-api",
  "version": "0.5.4",
  "description": "TypeScript library for interacting with the Bandcamp API",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    },
    "./schema/valibot": {
      "import": "./lib/schema/valibot.js",
      "require": "./lib/schema/valibot.cjs"
    },
    "./schema/zod": {
      "import": "./lib/schema/zod.js",
      "require": "./lib/schema/zod.cjs"
    }
  },
  "types": "./lib/index.d.ts",
  "files": [
    "lib/",
    "types/",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=18 || >=20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nutriot/bandcamp-api.git"
  },
  "keywords": [
    "bandcamp",
    "bandcamp-api"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "npm run build -- --watch",
    "fix": "eslint --fix ./src",
    "lint:code": "eslint **/*.json ./src/**/*.ts --no-warn-ignored",
    "lint:deps": "knip --production",
    "lint": "npm-run-all --parallel lint:*",
    "prepack": "npm run build",
    "prepare": "husky",
    "publish:npm": "np --no-yarn",
    "publish:jsr": "deno publish",
    "start": "npm run dev",
    "test": "uvu --require tsm tests"
  },
  "dependencies": {
    "valibot": "^0.30.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "eslint": "^9.10.0",
		"eslint-plugin-jsonc": "^2.16.0",
		"eslint-plugin-unicorn": "^55.0.0",
    "husky": "^9.1.6",
    "is-in-ci": "^0.1.0",
    "knip": "^5.30.2",
    "lint-staged": "^15.2.10",
    "log-symbols": "^6.0.0",
    "np": "^10.0.7",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "tslib": "^2.7.0",
    "tsm": "^2.3.0",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2",
		"typescript-eslint": "^8.6.0",
    "uvu": "^0.5.6"
  },
  "lint-staged": {
    "*.(json|js|ts)": "eslint --cache --fix",
    "*.md": "prettier --write"
  },
  "packageManager": "pnpm@9.1.2+sha512.127dc83b9ea10c32be65d22a8efb4a65fb952e8fefbdfded39bdc3c97efc32d31b48b00420df2c1187ace28c921c902f0cb5a134a4d032b8b5295cbfa2c681e2"
}
