{
  "name": "@nutriot/bandcamp-api",
  "version": "0.4.1",
  "description": "Node.js library for the Bandcamp API",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    },
    "./schema/valibot": {
      "import": "./lib/schema/valibot.js",
      "require": "./lib/schema/valibot.cjs"
    },
    "./schema/zod": {
      "import": "./lib/schema/zod.js",
      "require": "./lib/schema/zod.cjs"
    }
  },
  "types": "./lib",
  "files": [
    "dist/",
    "types/",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nutriot/bandcamp-api.git"
  },
  "keywords": [
    "bandcamp",
    "bandcamp-api"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:lib": "tsup src/index.ts --format esm,cjs --dts --out-dir lib",
    "build:schema": "tsup src/schema/*.ts --format esm,cjs --dts --out-dir lib/schema",
    "dev": "npm-run-all --parallel dev:*",
    "dev:lib": "npm run build:lib -- --watch",
    "dev:schema": "npm run build:schema -- --watch",
    "fix": "eslint --fix ./src",
    "lint:code": "eslint ./src --ignore-path .gitignore",
    "lint": "npm-run-all --parallel lint:*",
    "start": "npm run dev",
    "test": "ava ./tests/*.mjs --serial --verbose",
    "prepack": "npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "valibot": "^0.20.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/node": "^14.18.33",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "ava": "^4.3.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.46.0",
    "eslint-plugin-json": "^3.1.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "isomorphic-fetch": "^3.0.0",
    "lint-staged": "^13.2.3",
    "npm-run-all2": "^6.0.6",
    "prettier": "^2.7.1",
    "remark-cli": "^11.0.0",
    "remark-preset-lint-recommended": "^6.1.2",
    "remark-preset-prettier": "^2.0.1",
    "rimraf": "^3.0.2",
    "tslib": "^2.6.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "*.(json|js|ts)": "eslint --cache --fix",
    "*.md": "prettier --write"
  }
}
