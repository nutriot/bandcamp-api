{
  "name": "@nutriot/bandcamp-api",
  "version": "0.4.0",
  "description": "Node.js library for the Bandcamp API",
  "main": "lib/index.cjs",
  "exports": {
    "import": "./index.js",
    "require": "./index.cjs"
  },
  "files": [
    "lib/"
  ],
  "typings": "./types",
  "repository": {
    "type": "git",
    "url": "https://github.com/nutriot/bandcamp-api.git"
  },
  "keywords": [
    "bandcamp",
    "bandcamp-api"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rollup --config",
    "dev": "npm run start",
    "fix": "eslint --fix ./src",
    "lint:json": "jsonlint --quiet ./tools/*.json ./*.json",
    "lint:ts": "eslint ./src",
    "lint": "npm-run-all --parallel lint:*",
    "start": "rollup --watch --config",
    "test": "ava ./test/*.js --serial --verbose"
  },
  "dependencies": {},
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/node": "^14.14.19",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "ava": "^3.15.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.17.0",
    "eslint-plugin-json": "^2.1.2",
    "esm": "^3.2.25",
    "husky": "^4.3.6",
    "is-ci": "^2.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jsonlint": "^1.6.3",
    "lint-staged": ">=10",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.json": "jsonlint --quiet",
    "*.ts": "eslint --cache --fix"
  }
}
