{
  "name": "@nutriot/bandcamp-api",
  "version": "0.4.1",
  "description": "Node.js library for the Bandcamp API",
  "main": "lib/index.cjs",
  "exports": {
    "import": "./index.ms",
    "require": "./index.cjs"
  },
  "files": [
    "lib/"
  ],
  "typings": "./types",
  "repository": {
    "type": "git",
    "url": "https://github.com/nutriot/bandcamp-api.git"
  },
  "keywords": [
    "bandcamp",
    "bandcamp-api"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rollup --config",
    "dev": "npm run start",
    "fix": "eslint --fix ./src",
    "lint:code": "eslint ./src --ignore-path .gitignore",
    "lint": "npm-run-all --parallel lint:*",
    "start": "rollup --watch --config",
    "test": "ava ./test/*.mjs --serial --verbose",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/node": "^14.18.9",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "ava": "^4.0.1",
    "dotenv": "^14.2.0",
    "eslint": "^8.7.0",
    "eslint-plugin-json": "^3.1.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "isomorphic-fetch": "^3.0.0",
    "lint-staged": "^12.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "remark-cli": "^10.0.1",
    "remark-preset-lint-recommended": "^6.1.2",
    "remark-preset-prettier": "^1.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.66.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "*.(json|js|ts)": "eslint --cache --fix",
    "*.md": "prettier --write"
  }
}
